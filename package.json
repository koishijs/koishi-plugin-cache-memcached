    "impl:cache",
    "memcached",
    "memjs"
  ],
  "bugs": {
    "url": "https://github.com/koishijs/koishi-plugin-cache-memcached/issues"
  },
  "homepage": "https://github.com/koishijs/koishi-plugin-cache-memcached",
  "dependencies": {
